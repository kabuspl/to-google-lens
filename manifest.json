{

    "manifest_version": 3,
    "name": "To Google Lens",
    "version": "2.5",
    "permissions": ["menus", "activeTab", "scripting", "tabs"],

    "description": "Take a screenshot of a webpage and search on Google Lens.",

    "icons": {
        "32": "32.png",
        "128": "128.png"
    },

    "background": {
        "scripts": ["purify.min.js", "bg.js"]
    },

    "host_permissions": [
        "<all_urls>"
    ],

    "content_security_policy": {
        "extension_pages": "script-src 'self'; media-src *"
    }

}